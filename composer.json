    "keywords": ["console", "shared resource", "dependency injection"],
    "homepage": "https://github.com/OsLab/console-standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael COULLERET",
            "email": "michael@coulleret.pro",
            "role": "Lead Developer"
        },
        {
            "name": "Florent DESPIERRES",
            "email": "orions07@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
